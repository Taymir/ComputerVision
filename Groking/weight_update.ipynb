   "source": [
    "input = 2\n",
    "weight = 0.8\n",
    "goal = 2\n",
    "\n",
    "alpha = 0.1\n",
    "epochs = 20\n",
    "\n",
    "for i in range(epochs):\n",
    "    pred = input * weight\n",
    "    error = (pred - goal) ** 2\n",
    "    derivative = input * (pred - goal)\n",
    "    weight -= alpha * derivative\n",
    "    print (\"Error: %f, prediction: %f\" % (error, pred))\n",
    "        \n"
   ]
